@base <http://www.asiainfo.com/ontology/iot-fault-ticket#> .

<http://www.asiainfo.com/ontology/iot-fault-ticket#> rdf:type owl:Ontology ;
    owl:versionInfo "1.0.0" ;
    dcterms:title "物联网故障工单本体"@zh-CN ,
                   "IoT Fault Ticket Ontology"@en ;
    dcterms:description "用于描述物联网运营商故障工单管理的本体模型，涵盖工单生命周期、设备管理、故障诊断和数据分析"@zh-CN ;
    dcterms:creator "亚信科技 AI 研究院" ;
    dcterms:created "2025-01-08"^^xsd:date ;
    dcterms:license <http://www.apache.org/licenses/LICENSE-2.0> ;
    rdfs:comment "本体基于真实物联网运营商故障处理场景设计，提供标准化的概念模型和语义描述"@zh-CN .

#################################################################
#    对象属性 (Object Properties)
#################################################################

### http://www.asiainfo.com/ontology/iot-fault-ticket#describes
iotft:describes rdf:type owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:domain iotft:Ticket ;
    rdfs:range iotft:Fault ;
    rdfs:label "描述"@zh-CN ,
               "describes"@en ;
    rdfs:comment "工单所描述的故障"@zh-CN .

### http://www.asiainfo.com/ontology/iot-fault-ticket#reportedBy
iotft:reportedBy rdf:type owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:domain iotft:Ticket ;
    rdfs:range iotft:Customer ;
    rdfs:label "报告方"@zh-CN ,
               "reported by"@en ;
    rdfs:comment "报告工单的客户"@zh-CN .

### http://www.asiainfo.com/ontology/iot-fault-ticket#assignedTo
iotft:assignedTo rdf:type owl:ObjectProperty ;
    rdfs:domain iotft:Ticket ;
    rdfs:range iotft:Team ;
    rdfs:label "分配给"@zh-CN ,
               "assigned to"@en ;
    rdfs:comment "工单分配给的处理团队"@zh-CN .

### http://www.asiainfo.com/ontology/iot-fault-ticket#affects
iotft:affects rdf:type owl:ObjectProperty ;
    rdfs:domain iotft:Fault ;
