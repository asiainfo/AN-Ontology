#################################################################
#    类 (Classes)
#################################################################

### http://www.asiainfo.com/ontology/iot-fault-ticket#Ticket
iotft:Ticket rdf:type owl:Class ;
    rdfs:label "故障工单"@zh-CN ,
               "Fault Ticket"@en ;
    rdfs:comment "描述一个完整的故障处理工单，包括从创建到解决的全生命周期"@zh-CN ;
    rdfs:subClassOf skos:Concept ;
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty iotft:hasId ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] ;
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty iotft:hasStatus ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] ;
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty iotft:reportedBy ;
        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onClass iotft:Customer
    ] .

### http://www.asiainfo.com/ontology/iot-fault-ticket#Customer
iotft:Customer rdf:type owl:Class ;
    owl:equivalentClass foaf:Organization ;
    rdfs:label "客户"@zh-CN ,
               "Customer"@en ;
    rdfs:comment "使用物联网服务的客户组织"@zh-CN ;
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty iotft:customerName ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .

### http://www.asiainfo.com/ontology/iot-fault-ticket#Service
iotft:Service rdf:type owl:Class ;
    rdfs:subClassOf dcterms:Service ;
    rdfs:label "服务"@zh-CN ,
               "Service"@en ;
    rdfs:comment "提供给客户的物联网服务"@zh-CN .

### http://www.asiainfo.com/ontology/iot-fault-ticket#Device
iotft:Device rdf:type owl:Class ;
    rdfs:label "物联网设备"@zh-CN ,
               "IoT Device"@en ;
    rdfs:comment "使用物联网卡的终端设备"@zh-CN ;
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty iotft:deviceType ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] ;
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty iotft:uses ;
        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onClass iotft:SIMCard
    ] ;
    owl:disjointWith iotft:Fault , iotft:Ticket , iotft:Customer .

### http://www.asiainfo.com/ontology/iot-fault-ticket#SIMCard
iotft:SIMCard rdf:type owl:Class ;
    rdfs:label "物联网卡"@zh-CN ,
               "IoT SIM Card"@en ;
    rdfs:comment "用于物联网设备的SIM卡，包括实体卡和eSIM"@zh-CN ;
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty iotft:hasICCID ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] ;
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty iotft:hasMSISDN ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .

### http://www.asiainfo.com/ontology/iot-fault-ticket#Network
iotft:Network rdf:type owl:Class ;
    rdfs:label "网络"@zh-CN ,
               "Network"@en ;
    rdfs:comment "通信网络基础设施"@zh-CN .

### http://www.asiainfo.com/ontology/iot-fault-ticket#Fault
iotft:Fault rdf:type owl:Class ;
    rdfs:label "故障"@zh-CN ,
               "Fault"@en ;
    rdfs:comment "故障的抽象基类，所有具体故障类型都继承自此类"@zh-CN ;
    owl:disjointWith iotft:Ticket , iotft:Customer , iotft:Device .

### http://www.asiainfo.com/ontology/iot-fault-ticket#NetworkFault
iotft:NetworkFault rdf:type owl:Class ;
    rdfs:subClassOf iotft:Fault ;
    rdfs:label "网络故障"@zh-CN ,
               "Network Fault"@en ;
    rdfs:comment "与网络连接、信号覆盖相关的故障"@zh-CN .

### http://www.asiainfo.com/ontology/iot-fault-ticket#DeviceFault
iotft:DeviceFault rdf:type owl:Class ;
    rdfs:subClassOf iotft:Fault ;
    rdfs:label "设备故障"@zh-CN ,
               "Device Fault"@en ;
    rdfs:comment "设备硬件或软件相关的故障"@zh-CN .

### http://www.asiainfo.com/ontology/iot-fault-ticket#ConfigurationFault
iotft:ConfigurationFault rdf:type owl:Class ;
    rdfs:subClassOf iotft:Fault ;
    rdfs:label "配置故障"@zh-CN ,
               "Configuration Fault"@en ;
    rdfs:comment "配置错误导致的故障"@zh-CN .

### http://www.asiainfo.com/ontology/iot-fault-ticket#LocationDrift
iotft:LocationDrift rdf:type owl:Class ;
    rdfs:subClassOf iotft:Fault ;
    rdfs:label "位置漂移"@zh-CN ,
               "Location Drift"@en ;
    rdfs:comment "设备位置与实际位置不符的特殊故障类型"@zh-CN ;
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty iotft:actualLocation ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] ;
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty iotft:reportedLocation ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .

### http://www.asiainfo.com/ontology/iot-fault-ticket#FakeBaseStation
iotft:FakeBaseStation rdf:type owl:Class ;
